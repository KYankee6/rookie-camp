  <!-- ajaxs -->
  <script>
    var template = $("#card-target");
    console.log(template);
    function getCategories() {
      return function () {
        $.ajax({
          type: "GET",
          url: "/api/v1/category",
          dataType: "json",
          // contentType: 'application/json; charset=utf-8',
        })
          .done(function (res) {
            var html = "";
            res.forEach(function (item) {
              console.log(item);
              $(template)
                .find("[data-role=cat1]")
                .attr("href", "/product/" + item.id);
              $(template)
                .find("[data-role=cat2]")
                .attr("src", "/images/pd/" + item.img + ".png");
              $(template)
                .find("[data-role=cat3]")
                .attr("href", "/product/" + item.id);
              $(template).find("[data-role=cat3]").text(item.name);
              $(template).find("[data-role=cat4]").text(item.description);
              $(template).find("[data-role=cat5]").text(item.taglist);
              html += $(template).html();
            });
            $("#crow").append(html);
          })
          .fail(function (error) {
            alert(JSON.stringify(error));
          });
      };
    }
    $(document).ready(getCategories());
    var tabs = $(".category-tabs");
    $(".category-tabs").on("click", function () {
      for (const tab of tabs) {
        tab.classList.remove("label-blue-on");
      }
      this.classList.add("label-blue-on");
      $.ajax({
        type: "GET",
        url: "/api/v1/category/" + this.dataset.categoryId,
        dataType: "json",
        // contentType: 'application/json; charset=utf-8',
      })
        .done(function (res) {
          var html = "";
          res.forEach(function (item) {
            console.log(item);
            $(template)
              .find("[data-role=cat1]")
              .attr("href", "/product/" + item.id);
            $(template)
              .find("[data-role=cat2]")
              .attr("src", "/images/pd/" + item.img + ".png");
            $(template)
              .find("[data-role=cat3]")
              .attr("href", "/product/" + item.id);
            $(template).find("[data-role=cat3]").text(item.name);
            $(template).find("[data-role=cat4]").text(item.description);
            $(template).find("[data-role=cat5]").text(item.taglist);
            // copy = template;
            html += $(template).html();
          });
          $("#crow").find(".item").remove();
          $("#crow").append(html);
        })
        .fail(function (error) {
          alert(JSON.stringify(error));
        });
    });
  </script>
